{
    "config": "air",
    "compilerOptions": {
        "source-path": [
            "src",
            "code"
        ],
        "library-path": [
            "libs"
        ],
        "output": "bin/Main.swf"
    },
    "mainClass": "Main",
    "application": "src/Main-app.xml",
    "airOptions": {
        "files": [
            {
                "file": "assets",
                "path": "assets"
            }
        ],
        "output": "bin/Main.air",
        "signingOptions": {
            "storetype": "pkcs12",
            "keystore": "files/certificate.p12"
        }
    }
}